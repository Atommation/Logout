img = (
    (12,1,1,1),
    (4,1,1,1,6,1,),
    (5,1),
    (9,1),
    (8,3,5,2),
    (7,1,1,1,1,1,4,1),
    (2,3,1,7,1,3),
    (2,1,2,4,1,4),
    (1,2,1,3,1,3,1,3),
    (3,4,5,4),
    (2,15),
    (3,14),
    (6,1,5,1),
    (6,1,5,1),
    (5,2,5,2)
)

k = 0

for i in img:
    k = 0
    for j in i:
        if (k%2) == 0:
            print("  "*j,end="")
            k += 1
            continue
        if (k%2) == 1:
            print("**"*j,end="")
            k += 1
    print("")
